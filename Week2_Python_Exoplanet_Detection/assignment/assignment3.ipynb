    "#3. Fibonacci Series Calculator\n",
    "# for 20 terms\n",
    "\n",
    "n=int(input(\"Type the number of terms in integer:\"))\n",
    "\n",
    "a=0\n",
    "b=1\n",
    "print(a)\n",
    "print(b)\n",
    "\n",
    "for i in range(n-2):\n",
